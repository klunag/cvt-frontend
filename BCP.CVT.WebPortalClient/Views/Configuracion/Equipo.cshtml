<div class="card" style="margin-bottom:25px;">
    <div class="card-body">
        <div class="card-search">
            <fieldset class="fieldset">
                <legend class="legend">Filtro de búsqueda</legend>
                <div class="card-search-box">
                    <form action="javascript:void(0);">
                        <div class="row form-group">
                            <div class="col-md-10 col-xs-8 col-sm-9">
                                <div class="input-with-icon containerFiltro">
                                    <input id="txtBusEq" class="form-control input-thick pill input-with-icon-search" type="text" placeholder="Búsqueda por nombre del equipo...">
                                    <span class="icon icon-search input-icon"></span>
                                </div>
                            </div>
                            <div class="col-md-2 col-xs-4 col-sm-3">
                                <button id="btnBusEq" class="btn btn-primary btn-width-custom" onclick="buscarEquipo()">Buscar</button>
                            </div>
                        </div>
                        <div class="row gutter-xs form-group">
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Tipo equipo: </h5>
                                    <select id="cbFilTipoEq" name="cbFilTipoEq" class="form-control">
                                        <option value="-1">-- Todos --</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Descubrimiento: </h5>
                                    <select id="cbFilDes" name="cbFilDes" class="form-control">
                                        <option value="-1">-- Todos --</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>¿Excluido de cálculos?: </h5>
                                    <select id="cbFilExCal" name="cbFilExCal" class="form-control"></select>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Estado: </h5>
                                    <select id="ddlEstado" name="ddlEstado" class="form-control"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </fieldset>
        </div>
    </div>
</div>

<div class="panel panel-body text-right" style="background-color: transparent;" data-toggle="match-height">
    <div class="col-xs-12 col-md-12">
        @*<a class="btn btn-primary" href="javascript: void(0)" onclick="IrEquiposDesactivados()">
                <span class="count-text">Equipos desactivados</span>
            </a>*@
        @*<a class="btn btn-primary" href="javascript: void(0)" onclick="IrActualizarEquipos()">
            <span class="icon icon-file-excel-o"></span>
            <span class="count-text">Actualizar equipos</span>
        </a>
        <a id="btnImportar" class="btn btn-primary" href="javascript: void(0)" onclick="IrImportarEquipos()">
                <span class="icon icon-file-excel-o"></span>
                <span class="count-text">Importar equipos</span>
            </a>*@
        <a class="btn btn-primary" href="javascript: void(0)" onclick="ExportarInfo()">
            <span class="icon icon-external-link"></span>
            <span class="count-text">Exportar</span>
        </a>
        <a class="btn btn-primary" href="javascript: void(0)" onclick="ExportarInfoDetallado()">
                <span class="icon icon-external-link"></span>
                <span class="count-text">Exportar IPs/Otros</span>
            </a>
        <a class="btn btn-primary btn-width-custom" href="javascript: void(0)" onclick="AddEquipo()">
            <span class="icon icon-plus"></span>
            <span class="count-text">Nuevo</span>
        </a>
    </div>
</div>

<div class="row gutter-xs">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <strong>Registros de los equipos</strong>
            </div>
            <div class="card-body">
                <table id="tblEquipo" class="table" data-unique-id="Id" data-mobile-responsive="true" data-detail-view="false" data-detail-formatter="detailFormatter" data-row-style="rowStyle" style="font-size:11px !important;">
                    <thead>
                        <tr>
                            <th data-formatter="rowNumFormatterServer" data-valign="middle" data-align="center" data-width="2%">#</th>
                            <th data-formatter="linkFormatter" data-field="Nombre" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%">Nombre</th>
                            <th data-field="TemporalToString" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="FlagTemporal">Descubrimiento</th>
                            <th data-field="TipoEquipo" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="TipoEquipoId">Tipo equipo</th>
                            <th data-field="Ambiente" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="AmbienteId">Ambiente</th>
                            <th data-field="FlagExcluirCalculoStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="FlagExcluirCalculo">¿Excluido cálculos?</th>
                            <th data-field="UsuarioCreacion" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-sort-name="UsuarioCreacion" data-width="10%">Creado por</th>
                            <th data-field="Subsidiaria" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-sort-name="DominioServidorId" data-width="10%">Subsidiaria</th>
                            <th data-field="SistemaOperativo" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="SistemaOperativo">Sistema Operativo</th>
                            <th data-formatter="opciones" data-field="Opciones" data-halign="center" data-valign="middle" data-align="center" data-width="5%" class="opcionesStyle">Opciones</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

