@{
    ViewBag.Title1 = "Equipos desactivados";
    ViewBag.Title2 = "Equipos y aplicaciones";
    ViewBag.Title3 = "Equipos desactivados";
}
<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-multiselect.js"></script>
<style>
    .opcionesStyle {
        min-width: 120px;
    }
</style>
<div class="card" style="margin-bottom:25px;">
    <div class="card-body">
        <div class="card-search">
            <fieldset class="fieldset">
                <legend class="legend">Filtro de búsqueda</legend>
                <div class="card-search-box">
                    <form action="javascript:void(0);">
                        <div class="row form-group">
                            <div class="col-md-10 col-xs-8 col-sm-9">
                                <div class="input-with-icon containerFiltro">
                                    <input id="txtBusEq" name="txtBusEq" class="form-control input-thick pill input-with-icon-search" type="text" placeholder="Búsqueda por nombre del equipo...">
                                    <span class="icon icon-search input-icon"></span>
                                </div>
                            </div>
                            <div class="col-md-2 col-xs-4 col-sm-3">
                                <button id="btnBusEq" class="btn btn-primary btn-width-custom" onclick="RefrescarListado()">Buscar</button>
                            </div>
                        </div>
                        <div class="row gutter-xs form-group">
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Tipo equipo: </h5>
                                    <select id="cbFilTipoEquipo" name="cbFilTipoEquipo" class="form-control"></select>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Subsidiaria: </h5>
                                    <select id="cbFilSubsidiaria" name="cbFilSubsidiaria" class="form-control"></select>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </fieldset>
        </div>
    </div>
</div>

<div class="panel panel-body text-right" style="background-color: transparent;" data-toggle="match-height">
    <div class="col-xs-12 col-md-12">
        @*<a id="btnImportar" class="btn btn-primary" href="javascript: void(0)" onclick="IrImportarEquipos()">
                <span class="icon icon-file-excel-o"></span>
                <span class="count-text">Importar equipos</span>
            </a>*@
        <a class="btn btn-primary" href="javascript: void(0)" onclick="ExportarEquiposDesactivados()">
            <span class="icon icon-external-link"></span>
            <span class="count-text">Exportar</span>
        </a>
        @*<a class="btn btn-primary" href="javascript: void(0)" onclick="ExportarInfoDetallado()">
                <span class="icon icon-external-link"></span>
                <span class="count-text">Exportar Detallado</span>
            </a>*@
    </div>
</div>

<div class="row gutter-xs">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <strong>Registros de los equipos desactivados</strong>
            </div>
            <div class="card-body">
                <table id="tblEquiposDes" data-locale="es-SP" class="table table-striped table-nowrap dataTable" cellspacing="0" width="100%" data-unique-id="Id" data-mobile-responsive="true">
                    <thead>
                        <tr>
                            <th data-formatter="rowNumFormatterServer" data-valign="middle" data-align="center" data-width="2%">#</th>
                            <th data-field="Nombre" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%">Nombre</th>
                            <th data-field="TipoEquipo" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="TipoEquipoId">Tipo equipo</th>
                            <th data-field="FechaUltimoEscaneoCorrectoStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%">Fecha último escaneo correcto</th>
                            <th data-field="FechaUltimoEscaneoErrorStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%">Fecha último escaneo error</th>
                            <th data-field="FechaModificacionStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="20%" data-sort-name="FechaModificacion">Fecha modificación</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/app/equipoDesactivado.js?v1.2.2"></script>