<div class="panel panel-body text-right" style="background-color: transparent;" data-toggle="match-height">
    <a class="btn btn-primary btn-width-custom" href="javascript: void(0)" onclick="ExportarInfo()">
            <span class="icon icon-external-link"></span>
            <span class="count-text">Exportar</span>
        </a>
</div>

<div class="row gutter-xs">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <strong>Modificaciones registradas en la plataforma</strong>
            </div>
            <div class="card-body">
                <table id="tblHistoricoModificacion" class="table table-striped table-nowrap dataTable" cellspacing="0" width="100%" data-unique-id="Id" data-mobile-responsive="true">
                    <thead>
                        <tr>
                            <th data-formatter="rowNumFormatterServer" data-valign="middle" data-align="center" data-width="2%">#</th>
                            <th data-field="Entidad" data-halign="center" data-valign="middle" data-align="left" data-width="15%" data-sort-name="Entidad">Entidad</th>
                            <th data-field="EntidadClave" data-halign="center" data-valign="middle" data-align="left" data-width="8%" data-sort-name="EntidadId">Id asociado</th>
                            <th data-field="FechaOrdenFormato" data-halign="center" data-valign="middle" data-align="left" data-width="15%" data-sort-name="FechaOrden">Fecha de última actualización</th>
                            <th data-field="CreadoPor" data-halign="center" data-valign="middle" data-align="left" data-width="15%" data-sort-name="CreadoPor">Creado por</th>
                            <th data-field="FechaCreacionFormato" data-halign="center" data-valign="middle" data-align="left" data-width="15%" data-sort-name="FechaCreacion">Fecha de creación</th>
                            <th data-field="ModificadoPor" data-halign="center" data-valign="middle" data-align="left" data-width="15%" data-sort-name="ModificadoPor">Modificado por</th>
                            <th data-field="FechaModificacionFormato" data-halign="center" data-valign="middle" data-align="left" data-width="15%" data-sort-name="FechaModificacion">Fecha de modificación</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/app/historicoModificacion.js?v=1.0.0"></script>