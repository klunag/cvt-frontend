@{
    ViewBag.Title1 = "Logs de Procesos Job";
    ViewBag.Title2 = "Configuración";
    ViewBag.Title3 = "Logs de Procesos Job";
}

<style>
    .idAsociado {
        min-width: 180px;
    }

</style>

 <div class="card">
    <div class="card-body">
        <div class="card-search">
            <fieldset class="fieldset">
                <legend class="legend">Filtro de búsqueda</legend>
                <div class="card-search-box">
                    <form id="formFiltros">
                        <div class="row gutter-xs">
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Tipo de Proceso: </h5>
                                    <select id="cbFilAccion" name="cbFilAccion" class="form-control"></select>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <h5>Fecha de Proceso: </h5>
                                <div class="input-group date" id="divFechaProceso">
                                    <input type="text" id="txtFiltroFechaProceso" name="txtFiltroFechaProceso" class="form-control" value="@DateTime.Now.ToString("dd/MM/yyyy")" />
                                    <span class="input-group-addon">
                                        <span class="icon icon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Tipo de Mensaje: </h5>
                                    <select id="cboTipoMensaje" name="cboTipoMensaje" class="form-control"></select>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Capa: </h5>
                                    <select id="cboCapa" name="cboCapa" class="form-control"></select>
                                </div>
                            </div>
                        </div>
                        <div class="row gutter-xs">
                            <div class="col-md-12">
                                <div class="text-right">
                                    <input type="button" id="btnBuscar" name="btnBuscar" value="Buscar" class="btn btn-primary btn-width-custom" />
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </fieldset>
        </div>
    </div>
</div> 

<div class="panel panel-body text-right" style="background-color: transparent;" data-toggle="match-height">
    <a class="btn btn-primary btn-width-custom" href="javascript: void(0)" onclick="ExportarInfo()">
            <span class="icon icon-external-link"></span>
            <span class="count-text">Exportar</span>
        </a>
</div>

<div class="row gutter-xs">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <strong id="strtitletable"></strong>
            </div>
            <div class="card-body">
                <table id="tblData" class="table table-striped table-nowrap dataTable" cellspacing="0" width="100%" data-unique-id="Id" data-mobile-responsive="true">
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/app/logsprocesos.js?v=1.7.7"></script>