@{
    Layout = "~/Views/Shared/_EmptyLayout.cshtml";
    string subtitulo = (string)ViewBag.SubtituloVista;
    ViewBag.Title1 = "Gestión de Aplicaciones";
    ViewBag.Title2 = "Configuración";
    ViewBag.Title3 = string.Format("Gestión de Aplicaciones - {0}", subtitulo);
    int AplicacionId = (int)ViewBag.AplicacionId;
    int VistaId = (int)ViewBag.VistaId;
    int _idBandeja = (int)ViewBag.BandejaId;
    int paginaActual = (int)ViewBag.PaginaActual;
    int paginaTamanio = (int)ViewBag.PaginaTamanio;
    int idTipoSolicitud = (int)ViewBag.TipoSolicitudId;

    string controllerRedirect = string.Empty;
    string actionRedirect = string.Empty;
    if(VistaId == (int)EVistaGestionAplicacion.Registro)
    {
        controllerRedirect = "SolicitudCreacionAplicacion";
        actionRedirect = "creación";
    }
    if (VistaId == (int)EVistaGestionAplicacion.Modificar)
    {
        controllerRedirect = "SolicitudModificacionAplicacion";
        actionRedirect = "modificación";
    }
}

