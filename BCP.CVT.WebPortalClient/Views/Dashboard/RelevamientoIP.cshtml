
@{
    ViewBag.Title1 = "Relevamiento IP";
    ViewBag.Title2 = "Dashboard Obsolescencia - Equipos";
    ViewBag.Title3 = "Relevamiento IP";
}

<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-multiselect.js"></script>
<style>
    .estiloDefecto {        
        text-align: center;
        width: 10%;
    }
    .fondoAzul {
        background-color: #ffffff !important;
        text-align: center;
        vertical-align: middle !important;
        color: #001B45;
        width: 16% !important;
    }
    .fondoNaranja {
        background-color: #FF7800 !important;
        text-align: center;
        vertical-align: middle !important;
        color: #ffffff;
        width: 16% !important;
    }
    .fondoAmarillo {
        background-color: #FFDD5B !important;
        text-align: center;
        vertical-align: middle !important;
        width: 10% !important;
    }

    .fondoVerdeLeve {
        background-color: #91C337 !important;
        color: #ffffff !important;
        text-align: center;
        vertical-align: middle !important;
        width: 10% !important;
    }

    .fondoVerdeFuerte {
        background-color: #22A71C !important;
        color: #ffffff !important;
        text-align: center;
        vertical-align: middle !important;
        width: 10% !important;
    }
</style>
<div class="card">
    <div class="card-body">
        <div class="card-search">
            <fieldset class="fieldset">
                <legend class="legend">Filtro de búsqueda</legend>
                <form id="formFiltros">
                    <div class="card-search-box">
                        <div class="row gutter-xs">
                            <div class="col-xs-3 col-md-3">
                                <div class="text-left" data-toggle="match-height">
                                    <h5>Fecha de la consulta: (*)</h5>
                                    <div class="input-group date" id="divFechaFiltro">
                                        <input type="text" id="FechaFiltro" name="FechaFiltro" class="form-control" value="@DateTime.Now.ToString("dd/MM/yyyy")" />
                                        @*<span class="input-group-addon">
                                            <span class="icon icon-calendar"></span>
                                        </span>*@
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-3 col-md-3">

                            </div>
                            <div class="col-xs-3 col-md-3">
                                <h5>&nbsp;</h5>
                                    <button id="btnExportar" type="button" class="btn btn-primary btn-width-custom" onclick="ExportarInfo()">Exportar</button>
                            </div>
                            <div class="col-xs-3 col-md-3">
                                <h5>&nbsp;</h5>
                                <button id="btnBuscar" type="button" class="btn btn-primary btn-width-custom" onclick="Buscar()">Buscar</button>
                            </div>
                        </div>
                    </div>
                </form>
            </fieldset>
        </div>
    </div>
</div>
<div class="panel panel-body text-right" style="background-color: transparent;" data-toggle="match-height">
    
</div>

<div class="row gutter-xs">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <strong>Reporte de identificación de IPs</strong>
            </div>
            <div class="card-body">
                <table id="table" data-detail-view="true" class="table table-striped table-nowrap" cellspacing="0" style="width:100%; font-size: 11px !important; vertical-align:middle !important;">
                    <thead>
                        <tr></tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/app/relevamientoip.js?v=2.1.0"></script>