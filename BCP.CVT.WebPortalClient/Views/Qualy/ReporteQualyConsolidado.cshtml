<style>
    .opcionesStyle {
        width: 500px;
    }

    .fondoRojo {
        background-color: #E33D01 !important;
        color: #ffffff !important;
        text-align: center;
        width: 10%;
    }

    .fondoNaranja {
        background-color: #FF7F0D !important;
        color: #ffffff !important;
        text-align: center;
        width: 10%;
    }

    .fondoAmarillo {
        background-color: #FFDD5B !important;
        text-align: center;
        width: 10%;
    }

    .fondoVerdeLeve {
        background-color: #91C337 !important;
        color: #ffffff !important;
        text-align: center;
        width: 10%;
    }

    .fondoVerdeFuerte {
        background-color: #22A71C !important;
        color: #ffffff !important;
        text-align: center;
        width: 10%;
    }

    .fondoCeleste {
        background-color: #bfd9eb !important;
        color: #000000 !important;
        text-align: center;
        width: 10%;
    }

    .fondoBlanco {
        text-align: center;
        width: 10%;
    }

    .ui-autocomplete {
        max-height: 100px;
        overflow-y: auto;
        /* prevent horizontal scrollbar */
        overflow-x: hidden;
    }
