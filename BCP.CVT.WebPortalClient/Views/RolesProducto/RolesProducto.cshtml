                            <th data-field="Nombre" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-sort-name="Nombre" data-width="38%">Producto</th>
                            <th data-field="CantidadRoles" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-sort-name="CantidadRoles" data-width="10%">Roles </br>Asociados</th>
