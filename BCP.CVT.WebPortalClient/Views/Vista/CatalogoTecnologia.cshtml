                            <th data-field="CodigoProducto" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Código de<br />Producto</th>
                            <th data-field="ProductoStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Producto</th>
                            <th data-field="ClaveTecnologia" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Tecnología</th>
                            <th data-field="TipoTecnologiaStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" data-sort-name="Tipo">Estado de<br />Estandarización</th>
                            <th data-field="FechaCalculoValorTecStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" data-sort-name="FechaFinSoporte">Fecha fin<br />soporte</th>
                            <th data-field="EstadTecnologia" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" data-sort-name="EstadoId">Estado de<br />Tecnología</th>
                            <th data-formatter="formatNombreRoles" data-field="CantRoles" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200">Roles<br />Relacionados</th>
                            <th data-formatter="formatNombreFunciones" data-field="CantFunciones" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200">Funciones<br />Relacionadas</th>
                            <th data-field="TribuCoeStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Tribu/Coe/Unidad<br />Organizacional</th>
                            <th data-field="ResponsableTribuCoeStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Responsable de<br />Tribu/Coe/Unidad<br />Organizacional</th>
                            <th data-field="SquadStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Squad/Equipo</th>
                            <th data-field="ResponsableSquadStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Responsable<br />Squad/Equipo</th>
                            @*<th data-field="Dueno" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Responsable<br/>tecnología</th>*@
                            @*<th data-field="FuenteStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Fuente</th>*@
                            <th data-field="UrlConfluenceStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Descripción y Lineamientos Tecnológicos<br />(URL de Confluence)</th>
                            <th data-field="CasoUso" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Caso de uso</th>
                            <th data-field="Aplica" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Indicar la plataforma<br />a la que aplica</th>
                            <th data-field="CompatibilidadSOStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Compatibilidad<br />de SO</th>
                            <th data-field="CompatibilidadCloudStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Compatibilidad<br />Cloud</th>
                            <th data-field="ConfArqSeg" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Arquitecto<br />de seguridad<br />(Validador)</th>
                            <th data-field="ConfArqTec" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Arquitecto<br />tecnología<br />(Validador)</th>
                            <th data-field="GrupoSoporteRemedy" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Grupo de<br />Soporte REMEDY</th>
                            <th data-field="EsqLicenciamiento" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Esquema de<br />licenciamiento/subscripción</th>
                            <th data-field="EquipoAprovisionamiento" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Equipo de<br />Aprovisionamiento</th>
                            <th data-field="UsuarioCreacion" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200">Creado<br />Por</th>
                            <th data-field="FechaCreacionFormato" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" data-sort-name="FechaCreacion">Fecha<br />Creación</th>
                            <th data-field="UsuarioModificacion" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200">Modificado<br />Por</th>
                            <th data-field="FechaModificacionFormato" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" data-sort-name="FechaModificacion">Fecha<br />Modificación</th>
                            <th data-formatter="formatLBS" data-field="LineamientoBaseSeguridadStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-width="200" class="nombreDom">Linea Base<br />Seguridad</th>
                        </tr>
                    </thead>
                </table>
                <input type="hidden" name="hdEditFrom" id="hdEditFrom" value="" />
            </div>

        </div>
    </div>
</div>

