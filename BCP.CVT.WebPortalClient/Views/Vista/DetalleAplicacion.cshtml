@{

    ViewBag.Title1 = "Detalle por aplicación";
    ViewBag.Title2 = "Equipos y aplicaciones";
    ViewBag.Title3 = "Detalle por aplicación";
    string CodigoAPT = (string)ViewBag.CodigoAPT;
}
<style>
    .pad {
        padding-left: 0px;
        padding-right: 0px;
    }
</style>
<div class="view-detalle col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
        <div class="row"></div>
        <div class="x_title">
            <div class="pull-right" style="display: inline-flex">
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <div class="row"></div>
            <input type="hidden" id="hdId" name="hdId" value="0" />
            <div class="row">
                <div class="col-md-12">
                    <div class="col-md-7 pad">
                        <div class="col-md-12">
                            <div class="col-md-2 pad"><label>Código:</label></div>
                            <div class="col-md-10"><input type="text" id="txtCodApp" name="txtCodApp" class="form-control" readonly /></div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-2 pad"><label>Nombre:</label></div>
                            <div class="col-md-10"><input type="text" id="txtNombre" name="txtNombre" class="form-control" readonly /></div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-2 pad"><label>Criticidad:</label></div>
                            <div class="col-md-10"><input type="text" id="txtCriticidad" name="txtCriticidad" class="form-control" readonly /></div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-2 pad"><label>Gestionado Por:</label></div>
                            <div class="col-md-10"><input type="text" id="txtGestionadoPor" name="txtGestionadoPor" class="form-control" readonly /></div>
                        </div>
                        @*<div class="col-md-12">
                                <div class="col-md-4"><label>RoadMap:</label></div>
                                <div class="col-md-8"><input type="text" id="txtRoadMap" name="txtRoadMap" class="form-control" readonly /></div>
                            </div>*@
                        <div class="col-md-12">
                            <div class="col-md-2 pad"><label>Estado:</label></div>
                            <div class="col-md-10"><input type="text" id="txtEstado" name="txtEstado" class="form-control" readonly /></div>
                        </div>
                    </div>
                    <div class="col-md-3 pad">

                        <div class="col-md-12">
                            <label class="control-label">KPI Obsolescencia Forward Looking (%): <span class="icon icon-question-circle-o" data-toggle="tooltip" data-placement="right" title="Obsolescencia proyectada que considera lo vencido y lo por vencer"></span></label>
                            <br />
                        </div>
                        <div class="col-md-12 pad">
                            <div class="col-md-7">
                                <label>
                                    Situacion Actual:
                                </label>
                            </div>
                            <div class="col-md-5">
                                <input type="text" id="txtKPIObsolescencia" name="txtKPIObsolescencia" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-12 pad">
                            <div class="col-md-7">
                                <label id="lblKPIObsolescenciaProyeccion1">
                                </label>
                            </div>
                            <div class="col-md-5"><input type="text" id="txtKPIObsolescenciaProyeccion1" name="txtKPIObsolescenciaProyeccion1" class="form-control" readonly /></div>
                        </div>
                        <div class="col-md-12 pad">
                            <div class="col-md-7">
                                <label id="lblKPIObsolescenciaProyeccion2">
                                </label>
                            </div>
                            <div class="col-md-5"><input type="text" id="txtKPIObsolescenciaProyeccion2" name="txtKPIObsolescenciaProyeccion2" class="form-control" readonly /></div>
                        </div>

                        <div class="col-md-12 pad">
                            <div class="col-md-7">
                                <label id="lvlPCI">
                                    Tratamiento PCI DSS
                                </label>
                            </div>
                            <div class="col-md-5"><input type="text" id="txtPCI" name="txtPCI" class="form-control" readonly /></div>
                        </div>

                    </div>
                    <div class="col-md-2 pad">
                        <div class="col-md-12" style="padding-left:15px;padding-right:0px">
                            <label class="control-label">KPI Obsolescencia Real (%): <span class="icon icon-question-circle-o" data-toggle="tooltip" data-placement="left" title="Considera solo componentes vencidos"></span></label>
                            <br />
                        </div>
                        <div class="col-md-12 pad">
                            <div class="col-md-5">
                                <label>
                                    Situacion Actual:
                                </label>
                            </div>
                            <div class="col-md-7">
                                <input type="text" id="txtKPIObsolescenciaReal" name="txtKPIObsolescenciaReal" class="form-control" readonly />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12" style="margin-bottom:15px;">
                    @*<div class="col-md-4">
                            <label class="control-label">Servidores relacionados:</label>
                        </div>*@

                    <table id="tbl-servidoresRelacionadas" class="table" data-unique-id="Id">
                        <caption class="caption">
                            <span>Servidores relacionados</span>
                            <div class="pull-right" style="display: inline;">
                                <a class="btn btn-primary" onclick="ExportarServidoresRelacionados()">
                                    <span class="count-text">Exportar</span>
                                </a>
                            </div>
                        </caption>
                        <thead>
                            <tr>
                                <th data-field="tbl-servidoresRelacionadas" data-formatter="rowNumFormatterServerAlt" data-valign="middle" data-align="center" data-width="30">#</th>
                                <th data-formatter="EquipoLink" data-field="Nombre" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Equipo</th>
                                <th data-field="Ambiente" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Ambiente</th>
                                <th data-field="EstadoRelacionToString" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Estado de <br />la relación</th>
                                <th data-field="TipoEquipo" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">TipoEquipo</th>
                                <th data-field="SistemaOperativo" data-halign="center" data-valign="middle" data-align="left">Sistema Operativo</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="col-md-12" style="margin-bottom:15px;">
                    @*<label class="control-label">Certificados digitales relacionados:</label>*@
                    <table id="tbl-aplicacionCD" class="table" data-unique-id="Id">
                        <caption class="caption">
                            <span>Certificados Digitales (CA) relacionados</span>
                            <div class="pull-right" style="display: inline;">
                                <a class="btn btn-primary" onclick="ExportarCertificadosDigitalesRelacionadas()">
                                    <span class="count-text">Exportar</span>
                                </a>
                            </div>
                        </caption>
                        <thead>
                            <tr>
                                <th data-field="tbl-aplicacionCD" data-formatter="rowNumFormatterServerAlt" data-valign="middle" data-align="center" data-width="30">#</th>
                                <th data-field="Nombre" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Nombre</th>
                                <th data-field="TipoCertificado" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Tipo</th>
                                <th data-field="DescripcionTipoCertificado" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Descripción</th>
                                <th data-field="FechaCreacionCertificadoStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Fecha de creación</th>
                                <th data-field="FechaCalculoBaseStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Fecha de vencimiento</th>
                                <th data-formatter="semaforoFormatter" data-field="IndicadorObsolescencia" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado <br /> Actual</th>
                                <th data-formatter="semaforoIndice1Formatter" data-field="IndicadorObsolescencia_Proyeccion1" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado a <br /> <span id="cabMesesProyeccion1v2"></span> meses</th>
                                <th data-formatter="semaforoIndice2Formatter" data-field="IndicadorObsolescencia_Proyeccion2" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado a <br /> <span id="cabMesesProyeccion2v2"></span> meses</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="col-md-12" style="margin-bottom:15px;">
                    @*<label class="control-label">Client Secrets:</label>*@
                    <table id="tbl-aplicacionClientSecret" class="table" data-unique-id="Id">
                        <caption class="caption">
                            <span>Certificados Digitales & Client Secrets (Cloud) relacionados</span>
                            <div class="pull-right" style="display: inline;">
                                <a class="btn btn-primary" onclick="ExportarClientSecretRelacionadas()">
                                    <span class="count-text">Exportar</span>
                                </a>
                            </div>
                        </caption>
                        <thead>
                            <tr>
                                <th data-field="tbl-aplicacionClientSecret" data-formatter="rowNumFormatterServerAlt" data-valign="middle" data-align="center" data-width="30">#</th>
                                <th data-field="NombreClientSecret" data-halign="center" data-valign="middle" data-align="left">Nombre</th>
                                <th data-field="NombreCloud" data-halign="center" data-valign="middle" data-align="left">Cloud</th>
                                <th data-field="Suscripcion" data-halign="center" data-valign="middle" data-align="left">Suscripción</th>
                                <th data-field="TipoRecurso" data-halign="center" data-valign="middle" data-align="left">Resource Type</th>
                                <th data-field="Recurso" data-halign="center" data-valign="middle" data-align="left">Resource</th>
                                <th data-field="TipoClientSecret" data-halign="center" data-valign="middle" data-align="left">Tipo</th>
                                <th data-field="FechaCreacionStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Fecha de Inicio</th>
                                <th data-field="FechaFinSoporteStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Fecha de Vencimiento</th>
                                <th data-formatter="semaforoFormatter" data-field="IndicadorObsolescencia" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado <br /> Actual</th>
                                <th data-formatter="semaforoIndice1Formatter" data-field="IndicadorObsolescencia_Proyeccion1" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado a <br /> <span id="cabMesesProyeccion1v3"></span> meses</th>
                                <th data-formatter="semaforoIndice2Formatter" data-field="IndicadorObsolescencia_Proyeccion2" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado a <br /> <span id="cabMesesProyeccion2v3"></span> meses</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="col-md-12" style="margin-bottom:15px;">
                    @*<label class="control-label">Tecnologías relacionadas:</label>*@
                    <table id="tbl-aplicacionTecnologias" class="table" data-unique-id="Id">
                        <caption class="caption">
                            <span>Tecnologías relacionadas</span>
                            <div class="pull-right" style="display: inline;">
                                <a class="btn btn-primary" onclick="ExportarTecnologiasRelacionadas()">
                                    <span class="count-text">Exportar</span>
                                </a>
                            </div>
                        </caption>
                        <thead>
                            <tr>
                                <th data-field="tbl-aplicacionTecnologias" data-formatter="rowNumFormatterServerAlt" data-valign="middle" data-align="center" data-width="30">#</th>
                                <th data-field="DominioNomb" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Dominio</th>
                                <th data-field="SubdominioNomb" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Subdominio</th>
                                <th data-formatter="ClaveTecnologiaLink" data-field="ClaveTecnologia" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Tecnología</th>
                                <th data-field="TipoTecNomb" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Estado de estandarización</th>
                                <th data-field="EstadoStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Estado de tecnología</th>
                                <th data-field="FechaCalculoBaseStr" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Fecha de fin</th>
                                <th data-formatter="semaforoFormatter" data-field="IndicadorObsolescencia" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado <br /> Actual</th>
                                <th data-formatter="semaforoIndice1Formatter" data-field="IndicadorObsolescencia_Proyeccion1" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado a <br /> <span id="cabMesesProyeccion1"></span> meses</th>
                                <th data-formatter="semaforoIndice2Formatter" data-field="IndicadorObsolescencia_Proyeccion2" data-halign="center" data-valign="middle" data-align="center" data-sortable="false">Estado a <br /> <span id="cabMesesProyeccion2"></span> meses</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="col-md-12" style="margin-bottom:15px;">
                    <table id="tbl-apisRelacionadas" class="table" data-unique-id="Id">
                        <caption class="caption">
                            <span>Apis relacionadas</span>
                            <div class="pull-right" style="display: inline;">
                                <a class="btn btn-primary" onclick="ExportarApisRelacionadas()">
                                    <span class="count-text">Exportar</span>
                                </a>
                            </div>
                        </caption>
                        <thead>
                            <tr>
                                <th data-field="tbl-apisRelacionadas" data-formatter="rowNumFormatterServerAlt" data-valign="middle" data-align="center" data-width="30">#</th>
                                <th data-formatter="EquipoLink" data-field="Nombre" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Api</th>
                                <th data-field="Owner" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Owner</th>
                                <th data-field="Ambiente" data-halign="center" data-valign="middle" data-align="left">Ambiente</th>
                                <th data-field="EstadoRelacionToString" data-halign="center" data-valign="middle" data-align="left">Estado de <br />la relación</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="col-md-12" style="margin-bottom:15px;">
                    @*<label class="control-label">Usuarios responsables:</label>*@
                    <table id="tbl-aplicacionExpertos" class="table" data-unique-id="Id">
                        <caption class="caption">
                            <span>Usuarios responsables</span>
                            @*<div class="pull-right" style="display: inline;">
                                    <a class="btn btn-primary" onclick="ExportarTecnologiasRelacionadas()">
                                        <span class="count-text">Exportar</span>
                                    </a>
                                </div>*@
                        </caption>
                        <thead>
                            <tr>
                                <th data-field="tbl-aplicacionExpertos" data-formatter="rowNumFormatterServerAlt" data-valign="middle" data-align="center" data-width="30">#</th>
                                <th data-field="TipoExpertoToString" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Tipo</th>
                                <th data-field="Matricula" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Matrícula</th>
                                <th data-field="Nombres" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Nombres</th>
                                <th data-field="FechaCreacionFormato" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Fecha Creación</th>
                                <th data-field="UsuarioCreacion" data-halign="center" data-valign="middle" data-align="left" data-sortable="true">Creado Por</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    var CODIGOAPT = '@CodigoAPT';
</script>
<script src="~/app/detalleAplicacion.js?v1.2.8"></script>