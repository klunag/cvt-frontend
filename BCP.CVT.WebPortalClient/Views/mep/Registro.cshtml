
@{
    Layout = "~/Views/Shared/_MepLayout.cshtml";
    ViewBag.Title1 = "Registro";
    ViewBag.Title2 = "MEP";
    ViewBag.Title3 = "Registro";
}
<style>
    .bootstrap-table .fixed-table-container .table thead th .th-inner {
        overflow: inherit;
    }

    /*.table-header-rotated td.id {
        font-weight: bold;
    }*/

    /*.table-header-rotated th {
        height: 150px;
        white-space: nowrap;
    }*/
</style>

<div class="card">
    <div class="card-body">
        <div class="card-search">
            <fieldset class="fieldset" style="padding-block-start: 0 !important; padding-block-end: 0 !important;">
                <legend class="legend" style="margin-bottom: 0 !important;">Filtro de búsqueda</legend>
                <div class="card-search-box">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="text-left ui-widget tribuContainer" data-toggle="match-height">
                                <h5>Soportado por/Tribu: </h5>
                                <input type="text" id="txtTribuFiltro" name="txtTribuFiltro" value="" maxlength="100" autocomplete="on" class="form-control ui-autocomplete-input" />
                                <input type="hidden" name="hdTribuFiltro" id="hdTribuFiltro" value="0" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="text-left ui-widget aplicacionContainer" data-toggle="match-height">
                                <h5>Aplicación(código): </h5>
                                <input type="text" id="txtAplicacion" name="txtAplicacion" value="" maxlength="100" autocomplete="on" class="form-control ui-autocomplete-input" />
                                <input type="hidden" name="hdAplicacionFiltro" id="hdAplicacionFiltro" value="0" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="text-left ui-widget jdeContainer" data-toggle="match-height">
                                <h5>Jefe de Equipo/PO: </h5>
                                <input type="text" id="txtJdeFiltro" name="txtJdeFiltro" value="" maxlength="100" autocomplete="on" class="form-control ui-autocomplete-input" />
                                <input type="hidden" name="hdJdeFiltro" id="hdJdeFiltro" value="0" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="text-left ui-widget expertoContainer" data-toggle="match-height">
                                <h5>Experto/Especialista: </h5>
                                <input type="text" id="txtExpertoFiltro" name="txtExpertoFiltro" value="" maxlength="100" autocomplete="on" class="form-control ui-autocomplete-input" />
                                <input type="hidden" name="hdExpertoFiltro" id="hdExpertoFiltro" value="0" />
                            </div>
                        </div>
                    </div>
                    <div class="row gutter-xs">
                        <div class="col-md-12">
                            <div class="text-right">
                                <a id="btnBuscar" class="btn btn-primary" href="javascript: void(0)">
                                    <span class="icon icon-search"></span>
                                    <span class="count-text">Buscar</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>

<div class="panel panel-body text-right" style="background-color: transparent;" data-toggle="match-height">
    <a id="btnNuevo" class="btn btn-primary" href="javascript: void(0)">
        <span class="icon icon-plus"></span>
        <span class="count-text">Nuevo</span>
    </a>
    @*<a class="btn btn-primary btn-width-custom" href="javascript: void(0)" onclick="ExportarInfo()">
        <span class="icon icon-external-link"></span>
        <span class="count-text">Exportar</span>
    </a>*@
</div>

<div class="row gutter-xs">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <strong>Registros de Notas</strong>
            </div>
            <div class="card-body">
                <table id="tblRegistros" class="table table-striped table-nowrap" cellspacing="0" width="100%" data-mobile-responsive="true" style="font-size:10px;">
                    <thead>
                        <tr>
                            <th data-formatter="rowNumFormatterServer" data-valign="middle" data-align="center" data-width="2%">#</th>
                            <th data-field="TTL" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="15%">Tribu / Gestionado por</th>
                            <th data-field="CodigoAPT" data-halign="center" data-valign="middle" data-align="left" data-sortable="true" data-width="8%">Código</th>
                            <th data-field="NombreAplicacion" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-sort-name="NombreAplicacion" data-width="15%">Nombre</th>
                            <th data-field="JDE" data-halign="center" data-valign="middle" data-align="center" data-width="15%" data-sortable="true" data-sort-name="JDE">Jefe de Equipo</th>
                            <th data-field="DistribuidaStr" data-halign="center" data-valign="middle" data-align="center" data-width="5%" data-sortable="true" data-sort-name="Distribuida">MEP <br />Distribuida</th>
                            <th data-field="MainFrameStr" data-halign="center" data-valign="middle" data-align="center" data-width="5%" data-sortable="true" data-sort-name="MainFrame">MEP <br />Mainframe</th>
                            <th data-field="PaqueteSaasStr" data-halign="center" data-valign="middle" data-align="center" data-width="5%" data-sortable="true" data-sort-name="PaqueteSaas">MEP <br />Paquete/SaaS</th>
                            <th data-field="UserITMacroStr" data-halign="center" data-valign="middle" data-align="center" data-width="5%" data-sortable="true" data-sort-name="UserITMacro">UserIT <br /> Macro</th>
                            <th data-field="UserITWebStr" data-halign="center" data-valign="middle" data-align="center" data-width="5%" data-sortable="true" data-sort-name="UserITWeb">UserIT <br /> WEB</th>
                            <th data-field="UserITClientSeverStr" data-halign="center" data-valign="middle" data-align="center" data-width="5%" data-sortable="true" data-sort-name="UserITClientSever">UserIT <br /> Cli-Ser</th>
                            <th data-field="FechaModificacionStr" data-halign="center" data-valign="middle" data-align="center" data-sortable="true" data-sort-name="Activo" data-width="10%">Última <br />actualización</th>
                            <th data-formatter="opciones" data-field="Opciones" data-halign="center" data-valign="middle" data-align="center" data-width="5%">Acciones</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<!--Modal Add or Edit-->
<div id="mdAddOrEditRegistro" tabindex="-1" role="dialog" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header bg-primary">
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">×</span>
                    <span class="sr-only">Close</span>
                </button>
                <h4 class="modal-title" id="title-form"></h4>
            </div>
            <div class="modal-body">
                <form id="formAddOrEditRegistro">
                    <div class="form-group codigoAptContainer">
                        <label class="control-label">Aplicación (código): </label>
                        <input id="txtCodigoApt" name="txtCodigoApt" class="form-control" type="text">
                        <input type="hidden" name="hdAplicacionId" id="hdAplicacionId" value="0" />
                        <input type="hidden" name="hdRegistroId" id="hdRegistroId" value="0" />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nota MEP Distribuida: </label>
                        <div class="input-group">
                            <input id="txtDistribuida" name="txtDistribuida" class="form-control" type="text">
                            <span class="input-group-addon">%</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nota MEP MainFrame: </label>
                        <div class="input-group">
                            <input id="txtMainframe" name="txtMainframe" class="form-control" type="text">
                            <span class="input-group-addon">%</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nota MEP Paquete/SaaS: </label>
                        <div class="input-group">
                            <input id="txtPaqueteSaas" name="txtPaqueteSaas" class="form-control" type="text">
                            <span class="input-group-addon">%</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nota MEP UserIT Macro: </label>
                        <div class="input-group">
                            <input id="txtUserItMacro" name="txtUserItMacro" class="form-control" type="text">
                            <span class="input-group-addon">%</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nota MEP UserIT WEB: </label>
                        <div class="input-group">
                            <input id="txtUserItWeb" name="txtUserItWeb" class="form-control" type="text">
                            <span class="input-group-addon">%</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nota MEP UserIT Client/Sever: </label>
                        <div class="input-group">
                            <input id="txtUserItCliente" name="txtUserItCliente" class="form-control" type="text">
                            <span class="input-group-addon">%</span>
                        </div>
                    </div>
                    <input type="text" id="hdMinNota" name="hdMinNota" class="form-control" style="display:none;" />
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                <button id="btnRegistrar" class="btn btn-primary" type="button" data-loading-text="Procesando...">Registrar</button>
            </div>
        </div>
    </div>
</div>
<!--Modal Add or Edit-->
<script src="~/app/mepRegistro.js?v=1.1"></script>